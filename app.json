{
  "expo": {
    "name": "BarkEvents",
    "slug": "03",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/doglogo.png",
    "scheme": "03",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "The ultimate app for dog lovers to discover and create events for their furry friends!",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barkevent.app"
    },
    "android": {
      "package": "com.barkevent.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/doglogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "enableProguard": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/doglogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "40bf53a0-6f6e-47f6-9e02-3284e8af229a"
      }
    }
  }
}
